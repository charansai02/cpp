#include <iostream>
using namespace std;

bool isPrime(int num)
{
    if(num<=1)
    return false;
    else{
    for(int i=2; i<=num/2; i++)
    {
        if(num%i==0)
        return false;
        break;
    }
    return true;
    }
}
int main()
{
    int n;
    cout <<"Enter num: ";
    cin >> n;
    if(isPrime(n))
    cout << n <<" is Prime.";
    else
    cout << n <<" is not Prime.";
}
